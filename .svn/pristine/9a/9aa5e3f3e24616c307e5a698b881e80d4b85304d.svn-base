server.port=9000
spring.profiles.active=dev
logging.config=classpath:logback-spring.xml

spring.datasource.broker.jdbcUrl=jdbc:oracle:thin:@192.168.1.191:1421:MFTST
spring.datasource.broker.username=goex_webuser  
spring.datasource.broker.password=goex_webuser  
spring.datasource.broker.driverClassName=oracle.jdbc.OracleDriver

spring.datasource.portal.jdbcUrl=jdbc:postgresql://192.168.1.30/wp_uat
spring.datasource.portal.username=postgres  
spring.datasource.portal.password=postgres  
spring.datasource.portal.driverClassName=org.postgresql.Driver

mybatis.broker.mapperLocations=classpath:mapper/broker/*.xml
mybatis.portal.mapperLocations=classpath:mapper/portal/*.xml

spring.jackson.date-format=yyyy-MM-dd HH:mm:ss
spring.jackson.time-zone=GMT+8
spring.jackson.default-property-inclusion=always

spring.freemarker.template-loader-path=classpath:/templates/
spring.freemarker.cache=false
spring.freemarker.charset=UTF-8
spring.freemarker.check-template-location=true
spring.freemarker.content-type=text/html
spring.freemarker.expose-request-attributes=true
spring.freemarker.expose-session-attributes=true
spring.freemarker.request-context-attribute=request
spring.freemarker.suffix=.ftl